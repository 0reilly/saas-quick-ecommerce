{
  "name": "ecommerce-frontend",
  "version": "1.0.0",
  "description": "Ecommerce frontend application built with React, TypeScript, and TailwindCSS",
  "main": "index.js",
  "scripts": {
    "start": "pnpm run build && pnpm run serve",
    "build": "pnpm run tsc && pnpm run build:css && pnpm run build:js",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/globals.css --minify",
    "build:js": "tsc && tsc --module amd --outDir dist --removeComments --sourceMap --inlineSourceMap --inlineSources --declaration --emitDeclarationOnly",
    "serve": "vite",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "lint": "eslint . --fix",
    "ci:build": "pnpm run build",
    "test:unit:ci": "jest --coverage",
    "docker:build": "docker build . -t your-username/ecommerce-frontend:latest",
    "docker:push": "docker push your-username/ecommerce-frontend:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/ecommerce-frontend.git"
  },
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/ecommerce-frontend/issues"
  },
  "homepage": "https://github.com/your-username/ecommerce-frontend#readme",
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "cypress": "13.12.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.4",
    "postcss": "8.4.27",
    "prettier": "3.0.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "sharp": "0.32.4",
    "imagemin": "9.0.0"
  },
  "dependencies": {
    "axios": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.18.0",
    "@stripe/stripe-js": "^1.39.0",
    "@types/react-router-dom": "^5.3.3"
  },
  "pnpm": {
    "overrides": {
      "react-router-dom": "6.18.0"
    }
  }
}
